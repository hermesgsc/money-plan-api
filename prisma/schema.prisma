generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        Int       @id @default(autoincrement())
  name      String
  email     String    @unique
  password  String
  status    Status
  role      Role
  subscribe Subscribe
  createdAt DateTime  @default(now())
  updatedAt DateTime  @updatedAt
}

enum Status {
  active
  blocked
  deleted
  inative
}

enum Role {
  admin
  user
  support
  manager
}

enum Subscribe {
  basic
  pro
  vip
}
